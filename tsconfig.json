{
	"compilerOptions": {
        "module": "esNext",
		"target": "es5",
		"moduleResolution": "node",
		"strict": true,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "strictFunctionTypes": true,
        "skipLibCheck": true,
		"preserveConstEnums":true,
        "sourceMap": true,
        "rootDir": "src",
        
		"noImplicitAny": false,
        "strictNullChecks": false,
	}
}
